{
  "name": "hw-ledger-provider",
  "version": "1.1.35",
  "description": "Combined one package for interacting with Ledger device and Ledger API - Using LedgerJS , Ledger commons , Ledger common live ",
  "homepage": "http://coinagg.com",
  "scripts": {
    "prepack": "npm run prepack:babel && npm run prepack:flow",
    "prepack:babel": "babel provider/ -d lib",
    "prepack:flow": "flow-copy-source src lib",
    "ltcTest": "babel-node ./provider/test/ltctest.js",
    "ethTest": "babel-node ./provider/test/ethtest.js",
    "btcTest": "babel-node ./provider/test/btctest.js",
    "stellarTest": "babel-node ./provider/test/stellartest.js",
    "scan": "babel-node ./provider/tool/cli.js"
  },
  "keywords": [
    "Ledger",
    "LedgerJS",
    "LedgerCommon"
  ],
  "directories": {
    "lib": "./lib"
  },
  "main": "./lib/LedgerProvider.js",
  "files": [
    "lib"
  ],
